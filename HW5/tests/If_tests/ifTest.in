
void main(){
	int big = 100;
	int big2 = 100 b;
	byte small = 6 b;
	byte small2 = small;

	// ==: 1true	2true 
	if(big == big2){
		print("1true\t");
	} 
	if(small == small2){
		print("2true\t");
	} 
	if(big == small){
		print("3true\t");
	} 
	if(small == big){
		print("4true\t");
	} 
	print("\n");


	// !=: 3true 4true
	if(big != big2){
		print("1true\t");
	} 
	if(small != small2){
		print("2true\t");
	} 
	if(big != small){
		print("3true\t");
	} 
	if(small != big){
		print("4true\t");
	} 
	print("\n");

	// <: 4true
	if(big < big2){
		print("1true\t");
	} 
	if(small < small2){
		print("2true\t");
	} 
	if(big < small){
		print("3true\t");
	} 
	if(small < big){
		print("4true\t");
	} 
	print("\n");

	// <=: 1true 2true 4true
	if(big <= big2){
		print("1true\t");
	} 
	if(small <= small2){
		print("2true\t");
	} 
	if(big <= small){
		print("3true\t");
	} 
	if(small <= big){
		print("4true\t");
	} 
	print("\n");

	// >: 3true 
	if(big > big2){
		print("1true\t");
	} 
	if(small > small2){
		print("2true\t");
	} 
	if(big > small){
		print("3true\t");
	} 
	if(small > big){
		print("4true\t");
	} 
	print("\n");

	// >=: 1true 2true 3true 
	if(big >= big2){
		print("1true\t");
	} 
	if(small >= small2){
		print("2true\t");
	} 
	if(big >= small){
		print("3true\t");
	} 
	if(small >= big){
		print("4true\t");
	} 
	print("\n");
}
